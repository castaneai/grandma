{
    "manifest_version": 2,

    "name": "grandma",
    "description": "cookie editor for chrome",
    "version": "0.1",

    "browser_action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "cookies",
        "tabs",
        "*://*/*"
    ],

    "background": {
        "scripts": []
    }
}
